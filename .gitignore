# dependencies
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# testing
/coverage

# next.js
/.next/
/out/

# production
/build

# misc
.DS_Store
*.pem

# debug
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# env files (can opt-in for commiting if needed)
.env*

# vercel
.vercel

# typescript
*.tsbuildinfo
next-env.d.ts

# sqlite
# Ignore SQLite database files
*.sqlite
*.db
*.sqlite3
*.db3

# Ignore SQLite journal and transaction log files
*.sqlite-journal
*.db-journal
*.sqlite3-journal
*.db3-journal
*.sqlite-wal
*.sqlite-shm
*.db-wal
*.db-shm

# Ignore SQLite backups and temporary files
*.sqlite.bak
*.db.bak
*.sqlite-temp
*.db-temp
